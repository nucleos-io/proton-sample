{
  "extends": "airbnb-base",
  "globals": {
    "it": true,
    "proton": true,
    "User": true,
    "Token": true
  },
  "rules": {
    "func-names": 0,
    "semi": ["error", "never"],
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "new-cap": 0,
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "arrow-parens": 0
  }
}
